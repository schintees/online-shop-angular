{
  "name": "online-shop-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "i18n": {
    "sourceLocale": "en-US",
    "locales": {
      "ro": "apps/online-shop-angular/src/locale/messages.ro.xlf"
    }
  },
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/online-shop-angular/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "localize": [
          "ro"
        ],
        "outputPath": "dist/apps/online-shop-angular",
        "index": "apps/online-shop-angular/src/index.html",
        "main": "apps/online-shop-angular/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/online-shop-angular/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/online-shop-angular/src/favicon.ico",
          "apps/online-shop-angular/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/online-shop-angular/src/styles.scss"
        ],
        "scripts": [],
        "preserveSymlinks": true
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/online-shop-angular/src/environments/environment.ts",
              "with": "apps/online-shop-angular/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "online-shop-angular:build:production"
        },
        "development": {
          "browserTarget": "online-shop-angular:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "online-shop-angular:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "apps/online-shop-angular/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/online-shop-angular/src/favicon.ico",
          "apps/online-shop-angular/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/online-shop-angular/src/styles.scss"
        ],
        "scripts": []
      }
    },
    "ct": {
      "executor": "@cypress/schematic:cypress",
      "options": {
        "devServerTarget": "online-shop-angular:serve",
        "watch": true,
        "headless": false,
        "testingType": "component"
      },
      "configurations": {
        "development": {
          "devServerTarget": "online-shop-angular:serve:development"
        }
      }
    }
  }
}